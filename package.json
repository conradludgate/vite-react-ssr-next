{
  "name": "vite-react-ssr-next",
  "version": "0.0.0",
  "scripts": {
    "dev": "node server",
    "serve": "vite preview",
    "build": "yarn build:client && yarn build:server",
    "build:client": "tsc && vite build --outDir dist/static --ssrManifest && mv dist/static/index.html dist/index.html",
    "build:server": "tsc && vite build --ssr src/server.tsx --outDir dist/server",
    "generate": "yarn build && node prerender",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "@alpha",
    "react-dom": "@alpha",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.1",
    "@vitejs/plugin-react": "^1.0.0",
    "fast-glob": "^3.2.7",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  }
}
